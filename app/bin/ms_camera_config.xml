<?xml version="1.0" encoding="ISO-8859-1"?>

<Configuration version="1.0" >
<Devices>
    <!-- Base Devices -->

    <!-- Cameras -->
    <Device>
        <Type>AVTCamera</Type>
        <ID>cam1</ID>
        <DeviceID>116022</DeviceID>
        <Exposure>1000</Exposure>
    </Device>

    <Device>
        <Type>AVTCamera</Type>
        <ID>cam2</ID>
        <DeviceID>116021</DeviceID>
        <Exposure>1000</Exposure>
    </Device>

    <!-- Motors -->
    <Device>
        <Type>RotatoryMotor</Type>
        <ID>mscamrm1</ID>
        <DeviceID>1</DeviceID>
        <Port>/dev/ttyUSB1</Port>
        <Baudrate>9600</Baudrate>
    </Device>

    <Device>
        <Type>RotatoryMotor</Type>
        <ID>mscamrm2</ID>
        <DeviceID>2</DeviceID>
        <Port>/dev/ttyUSB2</Port>
        <Baudrate>9600</Baudrate>
    </Device>

    <!-- Compound Devices -->
    <Device>
        <Type>MSCamera</Type>
        <ID>mscam1</ID>
        <Camera>cam1</Camera>
        <Motor>mscamrm1</Motor>
        <Exposures>500 500 500 500 500 500 500 500 500 500</Exposures>
    </Device>

    <Device>
        <Type>MSCamera</Type>
        <ID>mscam2</ID>
        <Camera>cam2</Camera>
        <Motor>mscamrm2</Motor>
        <Exposures>500 500 500 500 500 500 500 500 500 500</Exposures>
    </Device>
</Devices>
</Configuration>
